{
  "presets": [
    [
      "@babel/preset-env",
      {
        targets: {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "shippedProposals": true
      }
    ],
    "@babel/preset-react"
  ],
  plugins: [
    "@babel/plugin-proposal-decorators",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "babel-plugin-transform-function-bind",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
